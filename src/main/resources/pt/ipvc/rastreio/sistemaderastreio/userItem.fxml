<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="983.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipvc.rastreio.sistemaderastreio.controller.UserItemController">
   <children>
      <Label fx:id="NameUser" prefHeight="22.0" prefWidth="196.0" text="Name">
         <font>
            <Font size="15.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="UsernameUser" prefHeight="22.0" prefWidth="183.0" text="Username">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="NumberPhoneUser" prefHeight="22.0" prefWidth="156.0" text="Number Phone">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="TypeUserUser" prefHeight="22.0" prefWidth="169.0" text="Type User">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="204.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" onAction="#editPass" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #43BD9B;" styleClass="button_" stylesheets="@Styles/user.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/forgotPass.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#edit" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #E2BA56;" styleClass="button_" stylesheets="@Styles/user.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/edit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#remove" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #EE8278;" styleClass="button_" stylesheets="@Styles/user.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/remove.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label fx:id="idUser" prefHeight="1.0" prefWidth="1.0">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</HBox>
