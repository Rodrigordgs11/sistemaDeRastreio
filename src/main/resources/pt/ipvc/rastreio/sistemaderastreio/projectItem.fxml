<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="983.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipvc.rastreio.sistemaderastreio.controller.ProjectItemController">
   <children>
      <Label fx:id="Name" prefHeight="22.0" prefWidth="133.0" text="Name">
         <padding>
            <Insets left="10.0" />
         </padding>
         <HBox.margin>
            <Insets right="14.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="ClientName" prefHeight="22.0" prefWidth="93.0" text="ClientName">
         <HBox.margin>
            <Insets right="14.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="Owner" prefHeight="22.0" prefWidth="104.0" text="Owner">
         <HBox.margin>
            <Insets right="14.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="TaskNumber" prefHeight="22.0" prefWidth="133.0" text="TaskNumber">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="Price" layoutX="662.0" layoutY="34.0" prefHeight="22.0" prefWidth="117.0" text="Price Per Hour">
         <HBox.margin>
            <Insets right="14.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="TimeSpent" layoutX="576.0" layoutY="34.0" prefHeight="22.0" prefWidth="88.0" text="TimeSpent">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <HBox alignment="CENTER_RIGHT" prefHeight="70.0" prefWidth="151.0" spacing="15.0">
         <children>
            <Button fx:id="lis" mnemonicParsing="false" onAction="#ListTasks" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #42bf2f;" styleClass="button_" stylesheets="@Styles/user.css" textFill="#42bf2f">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/icons8-list-30.png" />
                     </image>
                  </ImageView>
               </graphic>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#edit" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #E2BA56;" styleClass="button_" stylesheets="@Styles/user.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/edit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="remove" mnemonicParsing="false" onAction="#remove" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: #EE8278;" styleClass="button_" stylesheets="@Styles/user.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/remove.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <Label fx:id="idTask" text="idProject" />
   </children>
</HBox>
